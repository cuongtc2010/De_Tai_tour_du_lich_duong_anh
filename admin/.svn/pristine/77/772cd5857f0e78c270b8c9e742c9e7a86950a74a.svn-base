            $Ung_vien_nganh_ngheModel->setUngVienId($Id);
            $List = $Ung_vien_nganh_ngheModel->getList();
            $str = "";
            if (!is_null($List)) {
                foreach ($List as $value) {
                    $str .= $value["Id"] . ",";
                }
            }
            $str = rtrim($str, ",");

            echo $str;
        }
    }

