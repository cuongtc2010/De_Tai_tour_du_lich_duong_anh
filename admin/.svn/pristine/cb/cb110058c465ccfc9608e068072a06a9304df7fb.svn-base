        if (!is_null($this->getId())) {
            $sql .= " and goi_dich_vu.Id = '{$this->getId()}'";
        }
        if (!is_null($this->getNhaTuyenDungId())) {
            $sql .= " and NhaTuyenDungId='{$this->getNhaTuyenDungId()}'";
        }
        if (!is_null($this->getIsActive())) {
