    }

    public function insert() {
        $pdo = $this->getPDO();
        try {
            $pdo->beginTransaction();
            $sql = "insert into nha_tuyen_dung_goi_dich_vu(
                    `NhaTuyenDungId`,
                    `GoiDichVuId`,
                    `ApDungTu`,
                    `ApDungDen`,
                    SoLuongTinDang,
                    SoLuongUngVien,
                    SoLuongTuyenGap,
                    `IsActive`,
                    CreatedDate,
                    UpdatedDate,
                    CreatedBy,
                    UpdatedBy)
                     VALUES (
                    '{$this->getNhaTuyenDungId()}',
                    '{$this->getGoiDichVuId()}',
                    '{$this->getApDungTu()}',
                    '{$this->getApDungDen()}',
                    '{$this->getSoLuongTinDang()}',
                    '{$this->getSoLuongUngVien()}',
                    '{$this->getSoLuongTuyenGap()}',
                    '{$this->getIsActive()}',
                    now(),
                    now(),
                    '{$this->getCreatedBy()}',
                    '{$this->getUpdatedBy()}'

                    )";


            $pdo->exec($sql);
            $pdo->commit();
            return true;
        } catch (Exception $exc) {
            $pdo->rollBack();
            echo $exc->getTraceAsString();
        }
        return false;
    }

